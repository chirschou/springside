<?xml version="1.0" encoding="UTF-8"?>
<beans>
	<!-- 演示用途的最简单Server,不支持持久化,不支持JMX,在61616端口上侦听请求 -->
	<broker useJmx="false" persistent="false" xmlns="http://activemq.apache.org/schema/core">
		<transportConnectors>
			<transportConnector uri="tcp://localhost:61616" />
		</transportConnectors>
	</broker>
</beans>
